    def _activar_esfera_temporal(self):
        """Vigilancia en tiempo real"""
        print("â° Activando Esfera Temporal...")
        
        # Iniciar monitores en tiempo real
        monitores = [
            self._monitor_red,
            self._monitor_sistema,
