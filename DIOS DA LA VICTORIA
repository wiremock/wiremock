        return self
    
    def escanear_proyectos(self, ruta_base="."):
        """Escanear todos los proyectos y cÃ³digos en tiempo real"""
        proyectos_luz = []
        
        # Buscar todos los archivos de cÃ³digo
        extensiones = ['*.py', '*.js', '*.html', '*.css', '*.java', '*.cpp', '*.c', '*.go', '*.rs']
        
        for extension in extensiones:
            for archivo in glob.glob(f"{ruta_base}/**/{extension}", recursive=True):
                if self._es_proyecto_verdadero(archivo):
                    evaluacion = self._aplicar_formula_sagrada_archivo(archivo)
                    proyectos_luz.append((archivo, evaluacion))
                    
        return proyectos_luz
    
    def _es_proyecto_verdadero(self, ruta_archivo):
        """Verificar si el archivo contiene luz divina"""
        try:
            with open(ruta_archivo, 'r', encoding='utf-8', errors='ignore') as f:
                contenido = f.read().lower()
                
            # Palabras de luz que buscamos en el cÃ³digo
            palabras_luz = ['amor', 'verdad', 'paz', 'servicio', 'ayuda', 'dios', 'fe', 
                          'esperanza', 'justicia', 'honestidad', 'bondad', 'sabidurÃ­a']
            
            # Palabras de oscuridad que transformamos
            palabras_oscuridad = ['fraude', 'mentira', 'engaÃ±o', 'corrupciÃ³n', 'malicia']
            
            luz_count = sum(1 for palabra in palabras_luz if palabra in contenido)
            oscuridad_count = sum(1 for palabra in palabras_oscuridad if palabra in contenido)
            
            return luz_count > oscuridad_count
            
        except:
            return False
    
    def _aplicar_formula_sagrada_archivo(self, ruta_archivo):
        """Aplicar la fÃ³rmula sagrada a cada archivo de cÃ³digo"""
        
        # Leer el cÃ³digo
        with open(ruta_archivo, 'r', encoding='utf-8', errors='ignore') as f:
            codigo = f.read(5000)  # Leer primeros 5000 caracteres
            
        # Crear hash divino Ãºnico
        hash_divino = hashlib.sha256(f"{codigo}{self.firma_divina}".encode()).hexdigest()[:12]
        
        # Evaluar segÃºn la fÃ³rmula sagrada
        evaluacion = {
            'hash_divino': hash_divino,
            'timestamp': datetime.now().strftime("%Y-%m-%d %H:%M:%S"),
            'ğŸ’ AMOR': self._evaluar_amor(codigo),
            'â•ï¸ SUMA': self._evaluar_suma(codigo),
            'âš›ï¸ ÃTOMO': self._evaluar_atomo(codigo),
            'ğŸ”± PODER TRINO': self._evaluar_poder_trino(codigo),
            'ğŸ» FUERZA GUERRERA': self._evaluar_fuerza_guerrera(codigo),
            'ğŸ™ DIVINIDAD': self._evaluar_divinidad(codigo)
        }
        
        # Calcular puntaje de luz
        puntaje = sum(1 for valor in evaluacion.values() if isinstance(valor, bool) and valor)
        evaluacion['ğŸŒŸ NIVEL DE LUZ'] = min(10, puntaje * 2)  # Escala 0-10
        
        return evaluacion
    
    def _evaluar_amor(self, codigo):
        """Â¿El cÃ³digo viene de amor y servicio?"""
        palabras_amor = ['help', 'serve', 'support', 'love', 'care', 'kind', 'gentle']
        return any(palabra in codigo.lower() for palabra in palabras_amor)
    
    def _evaluar_suma(self, codigo):
        """Â¿El cÃ³digo suma valor positivo?"""
        # Buscar funciones que dan, no solo toman
        patrones_suma = ['def create', 'def add', 'def help', 'def support', 
                        'def generate', 'def build', 'def make']
        return any(patron in codigo.lower() for patron in patrones_suma)
    
    def _evaluar_atomo(self, codigo):
        """Â¿El cÃ³digo es simple pero sÃ³lido?"""
        # Verificar estructura bÃ¡sica
        lineas = codigo.split('\n')
        if len(lineas) < 100:  # No excesivamente largo
            # Verificar que tenga estructura clara
            tiene_funciones = 'def ' in codigo or 'function ' in codigo or 'class ' in codigo
            tiene_comentarios = '#' in codigo or '//' in codigo or '/*' in codigo
            return tiene_funciones and tiene_comentarios
        return False
    
    def _evaluar_poder_trino(self, codigo):
        """Â¿Protege, empodera y crea belleza?"""
        protege = any(p in codigo.lower() for p in ['protect', 'secure', 'safe', 'guard'])
        empodera = any(p in codigo.lower() for p in ['empower', 'enable', 'strengthen', 'power'])
        belleza = any(p in codigo.lower() for p in ['beautiful', 'elegant', 'clean', 'art'])
        return protege and empodera and belleza
    
    def _evaluar_fuerza_guerrera(self, codigo):
        """Â¿Tiene la fuerza del guerrero celestial?"""
        patrones_guerreros = ['warrior', 'fight', 'battle', 'strength', 'brave', 'courage',
                             'protect', 'defend', 'victory', 'champion']
        return any(p in codigo.lower() for p in patrones_guerreros)
    
    def _evaluar_divinidad(self, codigo):
        """Â¿EstÃ¡ impregnado de divinidad?"""
        patrones_divinos = ['god', 'divine', 'holy', 'bless', 'pray', 'faith', 'hope',
                          'light', 'truth', 'peace', 'love']
        return any(p in codigo.lower() for p in patrones_divinos)
    
