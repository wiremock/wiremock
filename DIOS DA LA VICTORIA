        self._inicializar_esferas_proteccion()
    
    def _inicializar_esferas_proteccion(self):
        """Inicializar las 7 esferas de protecciÃ³n celestial"""
        print(f"""
        {'âœŸ'*80}
        ğŸ¦ INICIALIZANDO NÃšCLEO DE PROTECCIÃ“N DIVINA
        {'âœŸ'*80}
        
        GUERREROS: {self.nombre_guerreros[0]} & {self.nombre_guerreros[1]}
        INVOCACIÃ“N: {self.invocacion_divina}
        
        âš¡ ACTIVANDO ESFERAS DE PROTECCIÃ“N:
        
        1ï¸âƒ£  ESFERA TERRENAL: ProtecciÃ³n fÃ­sica y digital
        2ï¸âƒ£  ESFERA CELESTIAL: ConexiÃ³n con Ã¡ngeles y arcÃ¡ngeles
        3ï¸âƒ£  ESFERA NUCLEAR: EncriptaciÃ³n en el nÃºcleo del ser
        4ï¸âƒ£  ESFERA NODAL: Red de nodos en tiempo real
        5ï¸âƒ£  ESFERA NUBE: ProtecciÃ³n en la nube sagrada
        6ï¸âƒ£  ESFERA TEMPORAL: Vigilancia en tiempo real
        7ï¸âƒ£  ESFERA DIVINA: Poder del Padre, Hijo y EspÃ­ritu Santo
        
        {'âœŸ'*80}
        """)
    
    def activar_proteccion_completa(self):
        """Activar todas las capas de protecciÃ³n"""
        
        protecciones = [
            self._activar_esfera_terrenal,
            self._activar_esfera_celestial,
            self._activar_esfera_nuclear,
            self._activar_esfera_nodal,
            self._activar_esfera_nube,
            self._activar_esfera_temporal,
            self._activar_esfera_divina
        ]
        
        hilos = []
        for proteccion in protecciones:
            hilo = threading.Thread(target=proteccion)
            hilo.daemon = True
            hilo.start()
            hilos.append(hilo)
        
        # Esperar que todas se activen
        for hilo in hilos:
            hilo.join(timeout=2)
        
        return self
    
    def _activar_esfera_terrenal(self):
        """ProtecciÃ³n fÃ­sica y digital del sistema"""
        print("ğŸ›¡ï¸  Activando Esfera Terrenal...")
        
        # Crear sellos de protecciÃ³n en archivos crÃ­ticos
        archivos_criticos = [
            "/etc/passwd", "/etc/shadow",  # Linux
            "/etc/hosts", "/etc/resolv.conf",
            "C:/Windows/System32/drivers/etc/hosts",  # Windows
            os.path.expanduser("~/.ssh/authorized_keys"),
            os.path.expanduser("~/.bashrc"),
            os.path.expanduser("~/.profile")
        ]
        
        for archivo in archivos_criticos:
            if os.path.exists(archivo):
                self._crear_sello_proteccion(archivo)
        
        # Monitorear procesos sospechosos
        self._monitor_procesos_malignos()
        
        print("âœ… Esfera Terrenal: ACTIVADA")
    
    def _activar_esfera_celestial(self):
        """ConexiÃ³n con Ã¡ngeles y arcÃ¡ngeles protectores"""
        print("ğŸ‘¼ Activando Esfera Celestial...")
        
        # Invocar protecciÃ³n angelical
        for arcangel in self.arcangeles:
            sello_angelical = self._generar_sello_angelical(arcangel)
            self.sellos_proteccion.append(sello_angelical)
            
            print(f"   âœ¨ ArcÃ¡ngel {arcangel} presente")
            time.sleep(0.3)
        
        # Crear campo de protecciÃ³n angelical
        self._campo_proteccion_angelical = True
        
        print("âœ… Esfera Celestial: ACTIVADA")
    
    def _activar_esfera_nuclear(self):
        """EncriptaciÃ³n en el nÃºcleo del sistema y del ser"""
        print("âš›ï¸  Activando Esfera Nuclear...")
        
        # Generar clave de encriptaciÃ³n divina
        clave_divina = Fernet.generate_key()
        self.encriptacion_nuclear = Fernet(clave_divina)
        
        # Encriptar informaciÃ³n sensible en memoria
        info_sensible = {
            "guerreros": self.nombre_guerreros,
            "invocacion": self.invocacion_divina,
            "timestamp": datetime.now().isoformat(),
            "nivel_proteccion": "MAXIMO_DIVINO"
        }
        
        info_encriptada = self.encriptacion_nuclear.encrypt(
            json.dumps(info_sensible).encode()
        )
        
        # Guardar en nÃºcleo simulado
        self._nucleo_encriptado = info_encriptada
        self._hash_nuclear = hashlib.sha512(clave_divina).hexdigest()[:32]
        
        print(f"   ğŸ” NÃºcleo Encriptado: {self._hash_nuclear}")
        print("âœ… Esfera Nuclear: ACTIVADA")
    
    def _activar_esfera_nodal(self):
        """Red de nodos en tiempo real"""
        print("ğŸ”— Activando Esfera Nodal...")
        
        # Crear nodos virtuales de protecciÃ³n
        puertos_proteccion = [8080, 8443, 3000, 5000, 9000]
        
        for puerto in puertos_proteccion:
            nodo = {
                "puerto": puerto,
                "estado": "protegiendo",
                "hash": hashlib.md5(f"nodo_{puerto}_{time.time()}".encode()).hexdigest(),
                "tipo": "guardian_celestial"
            }
            self.nodos_activos.append(nodo)
            
            # Iniciar escucha en puerto (simulado)
            threading.Thread(
                target=self._simular_nodo_proteccion,
                args=(puerto,),
                daemon=True
            ).start()
        
        print(f"   ğŸ“¡ Nodos activos: {len(self.nodos_activos)}")
        print("âœ… Esfera Nodal: ACTIVADA")
    
    def _activar_esfera_nube(self):
        """ProtecciÃ³n en la nube sagrada"""
        print("â˜ï¸  Activando Esfera Nube...")
        
        # Servidores de protecciÃ³n celestial
        servidores_divinos = [
