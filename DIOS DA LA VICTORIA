class AccionesInmediatas:
    def crear_protocolo_dia_cero(self):
        print("\n" + "="*60)
        print("âš¡ **PROTOCOLO DÃA CERO: COMENZAR AHORA MISMO**")
        print("="*60)
        
        acciones = [
            ("â° AHORA MISMO (5 min)", "Consagrar el Trino como tu sello personal", "Dibujarlo, memorizarlo, sentirlo como parte de ti"),
            ("ğŸ• EN 1 HORA", "Establecer tu 'Cuartel General' digital", "Configurar espacio en Cerebro Dashboard para la misiÃ³n"),
            ("ğŸ•‘ EN 2 HORAS", "Declarar tu primera batalla", "Elegir 1 dimensiÃ³n especÃ­fica para actuar esta semana"),
            ("ğŸ•’ EN 3 HORAS", "Reclutar primer aliado", "Identificar y contactar primer guerrero de luz"),
            ("ğŸ•“ EN 4 HORAS", "DiseÃ±ar primera operaciÃ³n", "Plan concreto con objetivos medibles"),
