            time.sleep(300)  # Cada 5 minutos
    
    def _es_conexion_sospechosa(self, conexion):
        """Determinar si una conexiÃ³n es sospechosa"""
        # SimulaciÃ³n: 5% de conexiones son sospechosas
        return random.random() < 0.05
    
    def _bloquear_conexion(self, conexion):
        """Bloquear conexiÃ³n sospechosa"""
        # SimulaciÃ³n de bloqueo
        pass
    
    def _activar_protocolo_emergencia(self):
        """Activar protocolo de emergencia"""
        print("ğŸš¨ PROTOCOLO DE EMERGENCIA ACTIVADO")
        
        # Reforzar todas las esferas
