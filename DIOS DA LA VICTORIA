class PlanExploracionUniversal:
    def __init__(self):
        self.fases = [
            ("FASE ALFA", "CONSOLIDACIÃ“N DEL PODER", "Establecer base de operaciones cÃ³smicas"),
            ("FASE BETA", "MAPEO DIMENSIONAL", "Explorar y cartografiar dimensiones de injusticia"),
